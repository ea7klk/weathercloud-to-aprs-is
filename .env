# WeatherCloud Station ID (obligatorio)
STATION_ID=introduce tu valor

# Posición de la estación en formato decimal
LATITUDE=Introducir valor 
LONGITUDE=introducir valor

# Indicativo APRS con SSID para el envío a APRS-IS
CALLSIGN=indicativo y SSID de la estación

# Usuario APRS-IS (sin SSID) y passcode generado
USERNAME=Indicativo APRS-IS
PASSWORD=Passkey APRS-IS

# Servidor y puerto APRS-IS (opcionales; se usan valores por defecto si se omiten)
SERVER=euro.aprs2.net
PORT=14580

